{
  "manifest_version": 3,
  "name": "Outline Toggler",
  "version": "1.0",
  "description": "Toggle outline for all elements on the page.",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  }
}
